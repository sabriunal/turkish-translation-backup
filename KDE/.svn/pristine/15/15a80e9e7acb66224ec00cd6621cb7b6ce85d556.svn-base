#: plasma_workspace.notifyrc:3
msgctxt "Comment"
msgid "Plasma Workspace"
msgstr "Plasma Çalışma Alanı"

#: plasma_workspace.notifyrc:56
msgctxt "Name"
msgid "Trash: Emptied"
msgstr "Çöp: Boşaltıldı"

#: plasma_workspace.notifyrc:107
msgctxt "Comment"
msgid "The trash has been emptied"
msgstr "Çöp boşaltıldı"

#: plasma_workspace.notifyrc:162
msgctxt "Name"
msgid "Textcompletion: Rotation"
msgstr "Metin tamamlama: Döndürme"

#: plasma_workspace.notifyrc:212
msgctxt "Comment"
msgid "The end of the list of matches has been reached"
msgstr "Eşleşme listesinin sonuna ulaşıldı"

#: plasma_workspace.notifyrc:266
msgctxt "Name"
msgid "Textcompletion: No Match"
msgstr "Metin tamamlama: Eşleşme yok"

#: plasma_workspace.notifyrc:316
msgctxt "Comment"
msgid "No matching completion was found"
msgstr "Eşleşen tamamlama bulunamadı"

#: plasma_workspace.notifyrc:370
msgctxt "Name"
msgid "Textcompletion: Partial Match"
msgstr "Metin tamamlama: Kısmi Eşleşme"

#: plasma_workspace.notifyrc:420
msgctxt "Comment"
msgid "There is more than one possible match"
msgstr "Birden fazla eşleşme var"

#: plasma_workspace.notifyrc:474
msgctxt "Name"
msgid "Fatal Error"
msgstr "Ölümcül Hata"

#: plasma_workspace.notifyrc:525
msgctxt "Comment"
msgid "There was a serious error causing the program to exit"
msgstr "Uygulamadan çıkılacak kadar önemli bir hata oluştu"

#: plasma_workspace.notifyrc:580
msgctxt "Name"
msgid "Notification"
