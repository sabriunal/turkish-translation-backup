#: config/appearance/kcm_ktp_chat_appearance.desktop:14
msgctxt "Name"
msgid "Chat Window Appearance"
msgstr "Sohbet Penceresi Görünüşü"

#: config/appearance/kcm_ktp_chat_appearance.desktop:62
msgctxt "Comment"
msgid "Choose a Style for your Chat Window and customize it"
msgstr "Sohbet Pencereniz için bir Biçem seçin ve yapılandırın"

#: config/behavior/kcm_ktp_chat_behavior.desktop:14
msgctxt "Name"
msgid "Chat Tab Behavior"
msgstr "Sohbet Sekme Davranışı"

#: config/messages/kcm_ktp_chat_messages.desktop:2
msgctxt "Name"
msgid "Plugins"
msgstr "Eklentiler"

#: config/messages/kcm_ktp_chat_messages.desktop:53
msgctxt "Comment"
msgid "How to deal with extra information obtained from messages"
msgstr "İletilerden alınan ek bilgiler nasıl anlaşılır"

#: config/otr/kcm_ktp_chat_otr.desktop:13
msgctxt "Comment"
msgid "Encrypt chat sessions with Off-The-Record encryption"
msgstr "Oturumları Kayıt Dışı şifrelemesi ile şifrele"

#: config/otr/kcm_ktp_chat_otr.desktop:53
#: filters/otr/ktptextui_message_filter_otr.desktop.cmake:2
msgctxt "Name"
msgid "OTR"
msgstr "OTR"

#: filters/bugzilla/ktptextui_message_filter_bugzilla.desktop.cmake:3
msgctxt "Name"
msgid "Bugzilla Information"
msgstr "Bugzilla Bilgisi"

#: filters/bugzilla/ktptextui_message_filter_bugzilla.desktop.cmake:49
msgctxt "Comment"
msgid ""
"When receiving a link to a bug on bugzilla show additional information such "
"as the title and status"
msgstr ""
"Bugzilla'daki bir hataya bağlantı alırken, başlık ve durum gibi ek bilgileri "
"göster"

#: filters/emoticons/kcm_ktptextui_message_filter_emoticons.desktop:9
#: filters/emoticons/ktptextui_message_filter_emoticons.desktop.cmake:2
msgctxt "Name"
msgid "Emoticons"
msgstr "İfadeler"

#: filters/emoticons/ktptextui_message_filter_emoticons.desktop.cmake:52
msgctxt "Comment"
msgid "Show emoticons (smiley faces) inside the chat"
msgstr "Sohbet içerisinde ifadeleri (gülen suratları) göster"

#: filters/formatting/ktptextui_message_filter_formatting.desktop.cmake:3
msgctxt "Name"
msgid "Text Formatting"
msgstr "Metin Biçimlendirme"

#: filters/formatting/ktptextui_message_filter_formatting.desktop.cmake:50
msgctxt "Comment"
msgid ""
"Use /, _, * and - to make text italic, underlined, bold or strikeout "
"respectively"
msgstr ""
"Metni eğik, kalın veya üstü çizili yapmak için sırasıyla /, _, * ve - "
"kullanın"

#: filters/geopoint/ktptextui_message_filter_geopoint.desktop.cmake:16
msgctxt "Name"
msgid "Geopoint Preview"
msgstr "Geopoint Önizlemesi"

#: filters/geopoint/ktptextui_message_filter_geopoint.desktop.cmake:53
msgctxt "Comment"
msgid "Show geo URI on a map view."
msgstr "Harita görünümüne coğrafi URI'yi göster."

#: filters/highlight/ktptextui_message_filter_highlight.desktop.cmake:17
msgctxt "Name"
msgid "Highlight"
msgstr "Vurgula"

#: filters/highlight/ktptextui_message_filter_highlight.desktop.cmake:65
msgctxt "Comment"
msgid ""
"If any incoming messages contain your nickname, it will highlight them in red"
msgstr ""
"Eğer takma adınızı içeren bir gelen ileti varsa kırmızı olarak vurgulanacak"

#: filters/images/ktptextui_message_filter_images.desktop.cmake:2
msgctxt "Name"
msgid "Image Preview"
msgstr "Resim Önizlemesi"

#: filters/images/ktptextui_message_filter_images.desktop.cmake:51
msgctxt "Comment"
msgid "Embed a thumbnail of any image URL sent in a chat"
msgstr "Sohbette gönderilmiş bir resim URL'sinin küçük resmini göm"

#: filters/latex/kcm_ktptextui_message_filter_latex.desktop:9
msgctxt "Name"
msgid "LaTeX"
msgstr "LaTeX"

#: filters/latex/ktptextui_message_filter_latex.desktop.cmake:17
msgctxt "Name"
msgid "LaTeX Renderer"
msgstr "LaTeX Sunucu"

#: filters/latex/ktptextui_message_filter_latex.desktop.cmake:62
msgctxt "Comment"
msgid ""
"If incoming messages contain LaTeX between $$, it will render it directly "
"into the chat."
msgstr ""
"Eğer gelen iletiler $$ arasında LaTeX içeriyorsa sohbette doğrudan sunulacak."

#: filters/otr/ktptextui_message_filter_otr.desktop.cmake:42
msgctxt "Comment"
msgid "If a message is an OTR message, it will not be shown"
msgstr "Bir ileti kayıt dışı ise gösterilmeyecektir"

#: filters/searchexpansion/ktptextui_message_filter_searchexpansion.desktop.cmake:2
msgctxt "Name"
msgid "Web Shortcuts"
msgstr "Web Kısa Yolları"

#: filters/searchexpansion/ktptextui_message_filter_searchexpansion.desktop.cmake:54
msgctxt "Comment"
msgid ""
"Expand a KDE-shortened URI into a URL (i.e. typing wp:KDE will translate to "
"http://en.wikipedia.org/wiki/KDE)"
msgstr ""
"KDE kısaltılmış bir URI'i URL'ye genişlet (örn. wp:KDE yazmak bunu http://en."
"wikipedia.org/wiki/KDE haline getirecektir)"

#: filters/texttospeech/ktptextui_message_filter_tts.desktop.cmake:2
msgctxt "Name"
msgid "Text To Speech"
msgstr "Konuşma"

#: filters/texttospeech/ktptextui_message_filter_tts.desktop.cmake:50
msgctxt "Comment"
msgid "Read out incoming messages using text-to-speech service"
msgstr "Konuşma hizmetiyle, gelen iletileri oku"

#: filters/urlexpansion/ktptextui_message_filter_urlexpansion.desktop.cmake:3
msgctxt "Name"
msgid "Short URLs"
msgstr "Kısa adresler"

#: filters/urlexpansion/ktptextui_message_filter_urlexpansion.desktop.cmake:47
msgctxt "Comment"
msgid "Shows the actual URL to which a shortened URL is redirecting to"
msgstr "Kısaltılmış URL'nin yönlendireceği asıl URL'yi gösterir"

#: filters/youtube/ktptextui_message_filter_youtube.desktop.cmake:2
msgctxt "Name"
msgid "YouTube Preview"
msgstr "YouTube Önizlemesi"

#: filters/youtube/ktptextui_message_filter_youtube.desktop.cmake:48
msgctxt "Comment"
msgid ""
"If any messages contains a link to a YouTube video, it will embed a preview "
"in the chat"
msgstr ""
"Eğer iletilerden biri bir YouTube video bağlantısı içeriyorsa sohbette "
"gömülü olarak önizlemesi olacak"

#: lib/ktptxtui_message_filter.desktop:4
msgctxt "Comment"
msgid "Telepathy Text-Ui Message Filter"
msgstr "Telepathy Metin Mesaj Süzgeci"

#: logviewer/config/kcm_ktp_logviewer_behavior.desktop:14
msgctxt "Name"
msgid "LogViewer Behavior"
msgstr "LogGörüntüleyici Davranışı"

#: logviewer/org.kde.ktplogviewer.desktop:2
msgctxt "Name"
msgid "KDE IM Log Viewer"
msgstr "KDE Anlık İletileşme Günlük Görüntüleyici"

#: logviewer/org.kde.ktplogviewer.desktop:51
msgctxt "GenericName"
msgid "KDE Instant Messenger Log Viewer"
msgstr "KDE Anlık İletileşme Günlük Görüntüleyici"

#: logviewer/org.kde.ktplogviewer.desktop:99
msgctxt "Comment"
msgid "Displays your KDE Instant Messenger logs"
msgstr "KDE Anlık İletileşme günlüklerinizi görüntüler"
