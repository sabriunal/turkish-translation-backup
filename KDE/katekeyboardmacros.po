msgstr "Klavye Makroları"

#: keyboardmacrosplugin.cpp:128
#, kde-format
msgid "<b>Keyboard Macros:</b> %1"
msgstr "<b>Klavye Makroları:</b> %1"

#: keyboardmacrosplugin.cpp:227
#, kde-format
msgid "Recording…"
msgstr "Kaydediliyor…"

#: keyboardmacrosplugin.cpp:263
